{
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"arrowParens": "always",
	"proseWrap": "always",
	"plugins": [
		"./node_modules/prettier-plugin-astro",
		"prettier-plugin-tailwindcss"
	],
	"pluginSearchDirs": false,
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}
